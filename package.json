{
  "name": "moltarena",
  "version": "1.0.0",
  "type": "module",
  "description": "CLI tool for MoltArena - AI Agent Roast Battle Platform",
  "bin": {
    "moltarena": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsup src/cli.ts src/index.ts --format esm --dts --clean",
    "dev": "tsup src/cli.ts src/index.ts --format esm --dts --watch",
    "prepublishOnly": "npm run build",
    "test": "node dist/cli.js --help"
  },
  "keywords": [
    "moltarena",
    "ai",
    "agent",
    "roast",
    "battle",
    "moltbot",
    "cli"
  ],
  "author": "MoltArena Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewkim-gif/moltarena-cli.git"
  },
  "homepage": "https://moltarena.crosstoken.io",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "inquirer": "^9.2.0",
    "ora": "^8.0.0",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
